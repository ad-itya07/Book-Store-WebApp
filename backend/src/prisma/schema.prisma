generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model book {
  id         Int    @id @default(autoincrement())
  title       String
  description String
  category    String
  trending    Boolean
  coverImage  String
  oldPrice    Float
  newPrice    Float
  createdAt   DateTime @default(now())
}
